{
  "name": "isometric",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "generate-icons": "npx pwa-asset-generator public/images/isometric.png  ./public/icons -i asset-generator/index.html -m public/manifest.webmanifest --portrait-only --background \"#0a1023\" --icon-only --padding \"0\"",
    "generate-splash": "npx pwa-asset-generator public/images/isometric.png  ./public/icons -i asset-generator/index.html -m public/manifest.webmanifest --portrait-only --background \"#0a1023\" --splash-only --padding \"35%\"",
    "generate-assets": "npm run generate-splash && npm run generate-icons"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@harelpls/use-pusher": "^7.2.1",
    "@prisma/client": "^4.2.1",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.1",
    "cupertino-pane": "1.2.82",
    "date-fns": "^2.29.2",
    "deep-equal": "^2.0.5",
    "focus-trap-react": "^9.0.2",
    "formik": "^2.2.9",
    "google-auth-library": "^8.3.0",
    "immer": "^9.0.15",
    "log4js": "^6.6.1",
    "next": "^13.2.2",
    "next-auth": "^4.10.3",
    "p-retry": "^5.1.1",
    "pusher": "^5.1.1-beta",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.9.1",
    "react-intersection-observer": "^9.4.0",
    "react-page-visibility": "^7.0.0",
    "react-transition-group": "^4.4.5",
    "react-use-websocket": "^3.0.0",
    "recharts": "2.1.12",
    "sass": "^1.54.5",
    "sharp": "^0.30.7",
    "swr": "^1.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.4",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "18.7.8",
    "@types/react": "18.0.17",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "18.0.6",
    "@types/react-page-visibility": "^6.4.1",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "eslint": "8.22.0",
    "eslint-config-next": "^13.2.2",
    "prisma": "^4.2.1",
    "typescript": "4.7.4"
  },
  "overrides": {
    "@harelpls/use-pusher": {
      "react": "^18"
    }
  }
}
